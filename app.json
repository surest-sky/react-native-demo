{
  "expo": {
    "name": "斑点熊",
    "slug": "dotdotbear",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/dotdotbear.png",
    "splash": {
      "image": "./assets/home.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dotdotbear.png",
        "backgroundColor": "#000000"
      },
      "package": "com.surest.reactAppExpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
